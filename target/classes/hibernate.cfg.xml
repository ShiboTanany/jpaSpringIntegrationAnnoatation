<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/contactList</property>
    <property name="hibernate.connection.username">root</property>
    <property name="hibernate.connection.password">root</property>
    <mapping class="com.mycompany.jpaspring1.Main"/>
    <mapping class="com.mycompany.jpaspring1.DaoInt"/>
    <mapping class="com.mycompany.jpaspring1.DaoCrud"/>
    <mapping class="com.mycompany.jpaspring1.ContactsInfo"/>
    <mapping class="com.mycompany.jpaspring1.User"/>
    <mapping class="com.mycompany.jpaspring1.entities.User"/>
    <mapping class="com.mycompany.jpaspring1.entities.ContactsInfo"/>
  </session-factory>
</hibernate-configuration>
